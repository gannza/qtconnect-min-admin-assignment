{
  "source": {
    "include": ["./src/"],
    "includePattern": "\\.(js|jsx)$",
    "excludePattern": "(node_modules/|docs/|dist/|tests/)"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
